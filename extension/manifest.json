{
  "manifest_version": 2,
  "name": "Gamify the Internet",
  "version": "1.0.0",
  "description": "Make the Internet fun again!",
  "icons": {
    "128": "icons/Icon_M.png",
    "48": "icons/Icon_S.png"
  },
  "browser_action": {
    "default_icon": "icons/Icon_M.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/jquery.min.js", "js/user_actions_min.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["js/stat_manage.js"]
  },
  "author": "Chris Germano",
  "permissions": [
    "storage"
  ]
}
